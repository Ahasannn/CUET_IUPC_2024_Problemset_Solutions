#include<bits/stdc++.h>
using namespace std;

#define FAST ios_base::sync_with_stdio(false); cin.tie(NULL)
using ll = long long;

void solve(int case_num) {
  ll n;
  cin >> n;

  if ((n == 2) || (n == 4)) cout << "SAME\n";
    else if (n == 1) cout << "POWER\n";
    else if (n == 3) cout << "SQUARE\n";
    else cout << "POWER\n";
}

int main() {
  FAST;

  int tc = 1;
  // cin >> tc;

  for (int ti = 1; ti <= tc; ++ti) {
    solve(ti);
  }
}